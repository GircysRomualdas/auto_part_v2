<h1>Manage All Customers</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>Created</th>
      <th>Updated</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.id %></td>
        <td><%= customer.email %></td>
        <td><%= customer.created_at %></td>
        <td><%= customer.updated_at %></td>
        <td><%= link_to "show", admin_customer_path(customer) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
